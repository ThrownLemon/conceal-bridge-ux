    "@angular/router": "^21.0.6",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "viem": "^2.43.3"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@playwright/test": "1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.3.0",
    "playwright-ng-schematics": "^21.0.3",
    "postcss": "^8.5.6",
